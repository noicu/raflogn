{
  "name": "@raflogn/core",
  "version": "0.0.1",
  "description": "Core logic of Raflogn, a graph editor / node editor for the web",
  "author": "noicu <1018496570@qq.com>",
  "homepage": "https://raflogn.no.icu",
  "license": "MIT",
  "files": [
    "dist/*"
  ],
  "main": "dist/cjs/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/esm/index.js",
  "scripts": {
    "build:esm": "tsc --declaration false --outDir dist/esm --module esnext",
    "build:cjs": "tsc --declaration false --outDir dist/cjs",
    "build:declaration": "tsc --emitDeclarationOnly --outDir dist",
    "build": "rimraf dist && yarn run build:esm && yarn run build:cjs && yarn run build:declaration",
    "test": "mocha -r ts-node/register \"test/**/*.spec.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noicu/raflogn.git"
  },
  "bugs": {
    "url": "https://github.com/noicu/raflogn/issues"
  },
  "dependencies": {
    "@raflogn/events": "^0.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/lodash.mapvalues": "^4.6.6",
    "@types/mocha": "^9.0.0",
    "@types/sinon": "^10.0.6",
    "@types/uuid": "^8.3.3",
    "chai": "^4.3.4",
    "mocha": "^9.1.3",
    "rimraf": "^3.0.2",
    "sinon": "^12.0.1",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.4"
  },
